        "b = int(input('nhap b = '))\n",
        "\n",
        "def uscln(a, b):\n",
        "    if (b == 0):\n",
        "        return a;\n",
        "    return uscln(b, a % b);\n",
        "print('UCLN của',a,'và',b,'là:',uscln(a,b))"
